{
  "scheme_version": 0.1,
  "sdk": {
    "disabled": false,
    "resource": {
      "attributes": {
        "service.name": "unknown_service"
      }
    },
    "propagators": [
      {
        "name": "tracecontext"
      },
      {
        "name": "baggage"
      },
      {
        "name": "b3"
      },
      {
        "name": "b3multi"
      },
      {
        "name": "b3multijaeger"
      },
      {
        "name": "xray"
      },
      {
        "name": "ottrace"
      }
    ],
    "attribute_limits": {
      "attribute_value_length_limit": 4096,
      "attribute_count_limit": 128
    },
    "tracer_provider": {
      "exporters": {
        "otlp": {
          "protocol": "http/protobuf",
          "endpoint": "http://localhost:4318/v1/traces",
          "certificate": "/app/cert.pem",
          "client_key": "/app/cert.pem",
          "client_certificate": "/app/cert.pem",
          "headers": {
            "api-key": 1234
          },
          "compression": "gzip",
          "timeout": 10000
        },
        "zipkin": {
          "endpoint": "http://localhost:9411/api/v2/spans",
          "timeout": 10000
        },
        "jaeger": {
          "protocol": "http/thrift.binary",
          "endpoint": "http://localhost:14268/api/traces",
          "timeout": 10000,
          "user": "user",
          "password": "password",
          "agent_host": "localhost",
          "agent_port": 6832
        },
        "console": {}
      },
      "span_processors": [
        {
          "name": "batch",
          "args": {
            "exporter": "console"
          }
        },
        {
          "name": "batch",
          "args": {
            "exporter": "otlp"
          }
        }
      ],
      "span_limits": {
        "attribute_value_length_limit": 4096,
        "attribute_count_limit": 128,
        "event_count_limit": 128,
        "link_count_limit": 128,
        "event_attribute_count_limit": 128,
        "link_attribute_count_limit": 128
      },
      "sampler_config": {
        "always_on": null,
        "always_off": null,
        "trace_id_ratio_based": {
          "ratio": 0.0001
        },
        "parent_based": {
          "root": "trace_id_ratio_based",
          "remote_parent_sampled": "always_on",
          "remote_parent_not_sampled": "always_off",
          "local_parent_sampled": "always_on",
          "local_parent_not_sampled": "always_off"
        },
        "jaeger_remote": {
          "endpoint": "http://localhost:14250",
          "polling_interval": 5000,
          "initial_sampling_rate": 0.25
        }
      },
      "sampler": "parent_based"
    },
    "meter_provider": {
      "exporters": {
        "otlp": {
          "protocol": "http/protobuf",
          "endpoint": "http://localhost:4318/v1/metrics",
          "certificate": "/app/cert.pem",
          "client_key": "/app/cert.pem",
          "client_certificate": "/app/cert.pem",
          "headers": {
            "api-key": 1234
          },
          "compression": "gzip",
          "timeout": 10000,
          "temporality_preference": "delta",
          "default_histogram_aggregation": "exponential_bucket_histogram"
        }
      },
      "metric_readers": [
        {
          "name": "periodic",
          "args": {
            "interval": 5000,
            "timeout": 30000,
            "exporter": "otlp"
          }
        },
        {
          "name": "prometheus",
          "args": {
            "host": "localhost",
            "port": 9464
          }
        }
      ],
      "views": [
        {
          "selector": {
            "instrument_name": "my-instrument",
            "instrument_type": "histogram",
            "meter_name": "my-meter",
            "meter_version": "1.0.0",
            "meter_schema_url": "https://opentelemetry.io/schemas/1.16.0"
          },
          "view": {
            "name": "new_instrument_name",
            "description": "new_description",
            "aggregation": {
              "name": "explicit_bucket_histogram",
              "args": {
                "boundaries": [
                  1.0,
                  2.0,
                  5.0
                ],
                "record_min_max": true,
                "max_size": 160
              }
            },
            "attribute_keys": [
              "key1",
              "key2"
            ]
          }
        }
      ]
    },
    "logger_provider": {
      "exporters": {
        "otlp": {
          "protocol": "http/protobuf",
          "endpoint": "http://localhost:4318/v1/logs",
          "certificate": "/app/cert.pem",
          "client_key": "/app/cert.pem",
          "client_certificate": "/app/cert.pem",
          "headers": {
            "api-key": 1234
          },
          "compression": "gzip",
          "timeout": 10000
        }
      },
      "log_record_processors": [
        {
          "name": "batch",
          "args": {
            "schedule_delay": 5000,
            "export_timeout": 30000,
            "max_queue_size": 2048,
            "max_export_batch_size": 512,
            "exporter": "otlp"
          }
        }
      ],
      "log_record_limits": {
        "attribute_value_length_limit": 4096,
        "attribute_count_limit": 128
      }
    }
  }
}
